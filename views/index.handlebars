<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <a class="navbar-brand" href="/">Home</a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav"
    aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active">
        <a class="nav-link" href="#">Orders <span class="sr-only">(current)</span></a>
      </li>
    </ul>
  </div>
</nav>
<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-5 offset-2">
        <h3>In process...</h3>
        <ul id="dish-list" class="list-group">
          {{#each dishes}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="dish/{{this.id}}">{{this.text}}</a>
          </li>
          {{/each}}
        </ul>
      </div>
      <div class="col-5 offset-2">
        <h3>Ready to pick up</h3>
        <ul id="dish-list" class="list-group">
          {{#each dishes}}
          {{#if this.ready}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="dish/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">Pick Up</button>
          </li>
          {{/if}}
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/index.js"></script>